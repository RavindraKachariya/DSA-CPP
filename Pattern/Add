CREATE TABLE Employee (
    emp_no INT PRIMARY KEY,
    emp_name VARCHAR(100),
    emp_department VARCHAR(50),
    emp_city VARCHAR(225),
    doj DATE
);

INSERT INTO Employee (emp_no, emp_name, emp_department, emp_city, doj)
VALUES
